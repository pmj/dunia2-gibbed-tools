<class name="#11E139C0">
	<inherit name="CEntityEvent" />
	<field name="text_sDetail" type="String" />
	<field name="sDetail" type="Hash32" />
	<field name="selType" type="Enum">
		<enum name="enumType">
			<element name="None">0</element>
			<element name="Bang">1</element>
			<element name="Snap">2</element>
			<element name="Pierce">3</element>
			<element name="Crush">4</element>
			<element name="Cut">5</element>
			<element name="Health">6</element>
			<element name="Burn">7</element>
			<element name="Water">8</element>
			<element name="Panic">9</element>
			<element name="Fear">10</element>
			<element name="Morale">11</element>
			<element name="Dirt">12</element>
			<element name="Unused_0">13</element>
			<element name="Interest">14</element>
			<element name="Stun">15</element>
			<element name="Explosion">16</element>
			<element name="Boom">17</element>
			<element name="Toxic">18</element>
			<element name="BleedOut">19</element>
		</enum>
	</field>
	<field name="selFilterSubType" type="Enum">
		<enum name="enumFilterSubType">
			<element name="None">0</element>
			<element name="ExplosionGrenade">1</element>
			<element name="ExplosionRPG">2</element>
			<element name="ExplosionIED">3</element>
			<element name="ExplosionSplash">4</element>
			<element name="Arrow">5</element>
			<element name="BurnPawnsOnly">6</element>
		</enum>
	</field>
	<field name="fLevel" type="Float32" />
	<field name="vectorImpactPos" type="Vector3" />
	<field name="fRawLevel" type="Float32" />
	<!--<object name="RadiusGroup">-->
		<field name="fRadius" type="Float32" />
		<field name="bFalloff" type="Boolean" />
		<field name="fFalloffMinLevel" type="Float32" />
		<field name="fFalloffInterpolationFactor" type="Float32" />
		<field name="bUseNewFalloffCurve" type="Boolean" />
		<field name="arkFalloffCurve" type="Id64" />
	<!--</object>-->
	<field name="hidShowType" type="Boolean" />
	<field name="hidShowRadius" type="Boolean" />
	<field name="fDuration" desc="Duration of effect (if valid)." type="Float32" />
</class>