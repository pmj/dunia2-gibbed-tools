<class name="STracerFX">
	<inherit name="#11E152C8" />
	<field name="text_texTexture" type="String" />
	<field name="texTexture" desc="Bullet tracer texture filename  " type="Hash64" />
	<field name="text_texTextureDistortion" type="String" />
	<field name="texTextureDistortion" desc="Bullet tracer distortion texture filename" type="Hash64" />
	<field name="text_texFacingTexture" type="String" />
